{
  "manifest_version": 3,
  "name": "downclocker",
  "version": "0",
  "description": "Emulate CPU throttling rate",
  "icons": {
    "128": "128.png"
  },
  "permissions": [
    "debugger",
    "tabs",
    "webNavigation"
  ],
  "background": {
    "service_worker": "bg.js"
  },
  "action": {
    "default_popup": "popup.htm"
  }
}